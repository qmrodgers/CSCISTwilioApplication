@model IEnumerable<PurchasedNumber>
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@

<table class="table">
        <thead>
            <th scope="col">Row #</th>
            <th scope="col">Purchased Number</th>
            <th scope="col">Friendly Name</th>
        </thead>
        <tbody>
            @{
                int count = 1;
                foreach (PurchasedNumber d in Model)
                {
                   
                    <tr>
                    <th scope="row">@count</th>
                    <td>@d.PurchasedNumberID</td>
                    <td>@d.FriendlyNumberName</td>
                    </tr>
                }
                }
        </tbody>
    </table>